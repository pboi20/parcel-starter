{
  "name": "parcel-starter",
  "version": "1.0.0",
  "description": "Front-end starter kit using the Parcel bundler",
  "main": "index.js",
  "dependencies": {
    "parcel-bundler": "^1.10.3"
  },
  "devDependencies": {
    "bootstrap": "^4.1.3",
    "imagemin": "^6.0.0",
    "imagemin-cli": "^3.0.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.5",
    "sass": "^1.15.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "parcel src/index.html",
    "build": "npm run build-main && npm run build-images",
    "build-main": "parcel build src/index.html --no-source-maps",
    "build-images": "imagemin dist/*.{jpg,png,gif,svg} -o dist/"
  },
